#! /usr/bin/env python

from __future__ import absolute_import

import argparse

from commands import reindex

parser = argparse.ArgumentParser(
    prog='git reindex',
    version='git-reindex 0.7.0',
    description=reindex.__doc__,
    epilog='for more detail, use: git help reindex'
)

parser.parse_args()
reindex.reindex()
